meta {
  name: login
  type: http
  seq: 1
}

post {
  url: {{api_url}}/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email": "{{api_user}}",
    "password": "{{api_password}}"
  }
}

script:post-response {
  let accessToken = res.body.access_token;
  
  bru.setVar("bearer_token",accessToken);
}
